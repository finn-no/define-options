{
  "name": "define-options",
  "version": "0.1.0",
  "description": "Define valid properties for an options litteral, and return a function that can validate the options",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run",
    "watch": "./node_modules/karma/bin/karma start",
    "umd": "./node_modules/.bin/umd defineOpts index.js dist/defineOpts.js",
    "min": "./node_modules/.bin/uglifyjs dist/defineOpts.js -o dist/defineOpts.min.js",
    "build": "npm run umd && npm run min"
  },
  "keywords": [
    "browser",
    "options",
    "validate",
    "util"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:finn-no/define-options.git"
  },
  "issues": "https://github.com/finn-no/define-options/issues",
  "author": "FINN.no AS",
  "license": "MIT",
  "devDependencies": {
    "es5-shim": "~2.3.0",
    "chai": "~1.9.1",
    "karma-chai": "~0.1.0",
    "mocha": "~1.18.2",
    "karma-mocha": "~0.1.3",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma": "~0.12.1",
    "karma-browserifast": "~0.4.1",
    "umd": "~2.0.0",
    "uglify-js": "~2.4.13"
  }
}
